{
  "name": "fef",
  "version": "0.0.9",
  "description": "Front-End Framework using react redux saga",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "prebuild": "eslint src test",
    "build:lib": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:lib && npm run build:es",
    "pretest": "npm run build",
    "test": "cross-env BABEL_ENV=test nyc mocha --require ./test/setup.js --no-timeouts",
    "prepublish": "npm test",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-band/fef.git"
  },
  "keywords": [
    "frontend",
    "framework",
    "react",
    "redux",
    "saga"
  ],
  "author": "d-band",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-band/fef/issues"
  },
  "files": [
    "es",
    "lib"
  ],
  "homepage": "https://github.com/d-band/fef#readme",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "global": "^4.3.0",
    "react-redux": "4.4.x",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.0"
  },
  "peerDependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-es2015-loose-native-modules": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.11.5",
    "coveralls": "^2.11.11",
    "cross-env": "^2.0.0",
    "eslint": "^3.1.1",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^6.0.0",
    "expect": "^1.20.2",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.0",
    "nyc": "^7.0.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  }
}